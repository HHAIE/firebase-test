{
    "debugMode": false,
    "webapp": {
        "port": "3090",
        "hostPort": "localhost:3090",
        "hostUrl": "https://localhost:3090",
        "folderPath": "/pages/",
        "loginStart": "pages/loginstart.html",
        "loginRedirect": "pages/logincomplete.html",
        "splashPage": "pages/splashpage.html",
        "splashPageTimeout": 2,
        "mainPage": "pages/index.html",
        "firstWidth": 1000,
        "firstHeight": 800,
        "persistentUser": true,
        "ContentSecurityPolicy": [
            "default-src 'self' ",
            "script-src 'self' 'nonce-3f98c07a4b' *.bootstrapcdn.com code.jquery.com *.google.com *.firebaseapp.com *.cloudfunctions.net *.youtube.com ",
            "img-src 'self' *.gstatic.com ",
            "style-src 'self' 'unsafe-inline' *.bootstrapcdn.com *.googleapis.com *.gstatic.com ",
            "font-src 'self' *.bootstrapcdn.com *.googleapis.com *.gstatic.com ",
            "connect-src 'self' *.google.com *.googleapis.com *.firebaseapp.com *.youtube.com ",
            "frame-src 'self' *.google.com *.googleapis.com *.firebaseapp.com "
        ]
    },
    "webFolders": [
        "pages",
        "scripts",
        "lib",
        "node_modules/firebase",
        "node_modules/firebaseui/dist"
    ],
    "apis": {
        "firebaseconfig": "/api/firebaseconfig",
        "logintoken": "/api/logintoken",
        "loginready": "/api/loginready"
    },
    "logout": {
        "google.com": "https://accounts.google.com/Logout",
        "facebook.com": "https://www.facebook.com/logout.php",
        "twitter.com": "https://twitter.com/logout",
        "github.com": "https://github.com/logout"
    },
    "providers": [
        "google.com",
        "github.com",
        "twitter.com",
        "facebook.com",
        "password",
        "phone"
    ]
}