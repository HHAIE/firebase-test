{
  "name": "electron-firebase",
  "productName": "Electron-Firebase Quickstart Cloud Framework",
  "version": "1.0.0",
  "description": "template app to demonstrate firebase authentication workflow in an electron app",
  "main": "electron-firebase.js",
  "config": {
    "loglevel": "error"
  },
  "scripts": {
    "start": "electron ./sample/main.js",
    "test": "electron ./tests/test_all_modules.js",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "cleanup": "npm prune && npm uninstall * && rm -rf ./node_modules",
    "gendocs": "node ./lib/gendocs.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.3.0",
    "body-parser": "^1.19.0",
    "electron": "^7.1.11",
    "electron-rebuild": "^1.10.0",
    "express": "^4.17.0",
    "fast-crc32c": "^2.0.0",
    "firebase": "^7.8.0",
    "firebaseui": "^4.0.0",
    "grpc": "^1.24.2",
    "helmet": "^3.21.2",
    "https": "^1.0.0",
    "request": "^2.88.0",
    "selfsigned": "^1.10.7",
    "url": "^0.11.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git@github.com:david-asher/electron-firebase.git"
  },
  "keywords": [
    "electron",
    "firebase",
    "authentication",
    "oauth",
    "mbaas"
  ],
  "author": "David Asher",
  "license": "MIT"
}
